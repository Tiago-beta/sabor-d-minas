{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "dist",
  "resolveJsonModule": true,
  "types": ["node"]
  },
  "include": ["src", "src/types.d.ts"],
  "exclude": ["node_modules"]
}
