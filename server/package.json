    "prisma": "^5.17.0",
    "@prisma/client": "^5.17.0",
    "@types/node": "^20.14.9"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "tsx": "^4.19.1",
    "eslint": "^9.9.0",
    "@typescript-eslint/parser": "^8.0.1",
